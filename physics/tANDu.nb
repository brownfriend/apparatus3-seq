Cell[CellGroupData[{

Cell["\<\
The interpolation functions are saved to a dat file, so that they can be used \
in the display physics program\
\>", "Subsection",
 CellChangeTimes->{{3.5691919716537757`*^9, 3.569192029108062*^9}}],

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"head", ",", "stream", ",", "dat"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"dat", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"v0", ",", " ", 
         RowBox[{"t", "[", "v0", "]"}], ",", 
         RowBox[{"uWannierFactor", "[", "v0", "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"v0", ",", "1.", ",", "40.", ",", "0.1"}], "}"}]}], "]"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"SetDirectory", "[", 
     RowBox[{"NotebookDirectory", "[", "]"}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{
    "head", "=", 
     "\"\<#v0\\tt(Er)\\tuWannierFactor(Er/latticeSpacing)\\n\>\""}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"stream", "=", 
     RowBox[{"OpenWrite", "[", "\"\<tANDU.dat\>\"", "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"WriteString", "[", 
     RowBox[{"stream", ",", 
      RowBox[{"head", "<>", 
       RowBox[{"ExportString", "[", 
        RowBox[{"dat", ",", "\"\<Table\>\""}], "]"}]}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Close", "[", "stream", "]"}], ";"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.567785047933148*^9, 3.567785096027018*^9}, {
