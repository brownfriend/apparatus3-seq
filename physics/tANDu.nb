      RowBox[{"latticeSpacing", "=", 
       RowBox[{"0.532", " ", 
        SuperscriptBox["10", 
         RowBox[{"-", "6"}]]}]}], ",", "aInLattSpacing"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"aInLattSpacing", "=", 
      RowBox[{"as", " ", 
       FractionBox["bohrRadius", "latticeSpacing"]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"aInLattSpacing", " ", 
      RowBox[{"uWannierFactor", "[", "v0", "]"}]}]}]}], "]"}]}]], "Input",
