Cell[CellGroupData[{

Cell["\<\
U as a function of magnetic field:  The scattering length is obtained using \
the formula from Innsbruck\
\>", "Subsection",
 CellChangeTimes->{{3.5592395592524796`*^9, 3.559239578914604*^9}, {
  3.5592405117129574`*^9, 3.559240514753131*^9}}],

Cell["\<\
bohr radius = 5 e-11
lattice spacing = 5 e-7
need 10,000 a0 to get scattering length equal to the lattice spacing\
\>", "Text",
 CellChangeTimes->{{3.5592403257173185`*^9, 3.559240368642774*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"a12", "[", "B_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"ab", "=", 
       RowBox[{"-", "1405."}]}], ",", 
      RowBox[{"b0", "=", "834.149"}], ",", 
      RowBox[{"d", "=", "300."}], ",", 
      RowBox[{"\[Alpha]", "=", "0.00040"}], ",", "aInBohrRadii", ",", 
      "bohrRadius", ",", "latticeSpacing", ",", "aInLattSpacing"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"aInBohrRadii", "=", 
      RowBox[{"ab", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         FractionBox["d", 
          RowBox[{"B", "-", "b0"}]]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"\[Alpha]", 
          RowBox[{"(", 
           RowBox[{"B", "-", "b0"}], ")"}]}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"bohrRadius", "=", 
      RowBox[{"5.29", " ", 
       SuperscriptBox["10", 
        RowBox[{"-", "11"}]]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"latticeSpacing", "=", 
      FractionBox[
       RowBox[{"1.064", " ", 
        SuperscriptBox["10", 
         RowBox[{"-", "6"}]]}], "2"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"aInLattSpacing", "=", 
      RowBox[{"aInBohrRadii", " ", 
       FractionBox["bohrRadius", "latticeSpacing"]}]}], ";", 
     "\[IndentingNewLine]", "aInLattSpacing"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.511136598586125*^9, 3.511136719492375*^9}, {
   3.51113675591425*^9, 3.511136784367375*^9}, {3.5223479617714396`*^9, 
   3.5223479697698965`*^9}, {3.5592396758841505`*^9, 
   3.5592398801228323`*^9}, {3.5592399533550205`*^9, 3.559239978204442*^9}, {
   3.559240023612039*^9, 3.559240051324624*^9}, {3.5592401683163157`*^9, 
   3.5592401729855833`*^9}, 3.5592402534361844`*^9, {3.5592402842029448`*^9, 
   3.5592402850829945`*^9}, {3.55924056176582*^9, 3.5592405964998064`*^9}}]
}, Open  ]],

